    "db:studio": "prisma studio",
    "db:generate": "prisma generate",
    "db:seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "dotenv": "^16.4.5",
    "express": "^4.19.2"
  },
  "devDependencies": {
    "@config/eslint": "workspace:*",
    "@types/express": "^4.17.21",
    "@types/node": "^20.12.12",
    "eslint": "^8.57.0",
    "prisma": "^5.22.0",
    "tsx": "^4.11.0",
    "typescript": "^5.4.5"
  }
}
